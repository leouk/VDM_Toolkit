types
	--TCRecordType,TCField,TCFieldList
	R :: 
		x: nat 
		y: nat	
	inv r == r.x < r.y;
	
	--Isabelle does not allow empty records
	--S :: ;
	
functions
	-- Auxiliary to allow for TCApplicationExpression of TCRecordType result for mkr(1).x
	mkr: nat -> R
	mkr(n) == mk_R(n,n+1);
	
	--TCRecordBind
	recbind: R -> nat
	recbind(mk_R(x,y)) == x+y;
	
